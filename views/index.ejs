        <div class="row justify-content-center">
            <h1 class="text-center">Upvote this image</h1>
        </div>
        <div class="row">
            <div class="col-md-12">
                <img src="https://media1.tenor.com/images/d9b79552094d5036f686f194d006d8d4/tenor.gif?itemid=15721111">
                <div class="voteBtns justify-content-center">
                    <button id="upvote" class="btn flex-item">⬆</button>
                    <p><span id="counter"></span></p>
                    <button id="downvote" class="btn flex-item">⬇</button>
                    <h5 id="myPopup">Upvote or walter will sniff you</h5>
                </div>
            </div>
        </div>
    </div>

    <script src="script.js"></script>
